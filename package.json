{
  "name": "@lifeart/ember-language-server",
  "description": "A language server implementation for Ember.js projects.",
  "version": "2.28.0",
  "contributors": [
    "Aleksandr Kanunnikov <lifeart92@gmail.com>"
  ],
  "license": "MIT",
  "keywords": [
    "ember",
    "language-server",
    "ember-language-server"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lifeart/ember-language-server.git"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": "bin/ember-language-server.js",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "resolutions": {
    "@glimmer/syntax": "0.84.1",
    "@babel/parser": "^7.17.0",
    "@babel/traverse": "^7.17.0",
    "@babel/core": "^7.17.0",
    "minimist": "1.2.6",
    "ansi-regex": "5.0.1",
    "lodash": "4.17.21",
    "find-up/locate-path": "7.1.0",
    "find-up/locate-path/p-locate": "6.0.0",
    "find-up/locate-path/p-locate/p-limit": "^4.0.0",
    "find-up/locate-path/p-locate/p-limit/yocto-queue": "^1.0.0",
    "find-up/path-exists": "5.0.0"
  },
  "dependencies": {
    "@glimmer/syntax": "0.84.1",
    "@lifeart/ember-extract-inline-templates": "2.2.0",
    "dag-map": "2.0.2",
    "ember-meta-explorer": "1.2.0",
    "ember-template-recast": "6.1.3",
    "find-up": "6.3.0",
    "flat": "5.0.2",
    "fuzzaldrin": "2.1.0",
    "json-parse-ast": "1.0.8",
    "lodash": "4.17.21",
    "memoizee": "0.4.15",
    "picomatch": "2.3.1",
    "semver": "7.3.5",
    "vscode-jsonrpc": "6.0.0",
    "vscode-languageserver": "7.0.0",
    "vscode-languageserver-protocol": "3.16.0",
    "vscode-languageserver-textdocument": "1.0.4",
    "vscode-uri": "3.0.3",
    "yaml": "2.0.0-11"
  },
  "devDependencies": {
    "@babel/types": "^7.12.12",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/cross-spawn": "^6.0.2",
    "@types/estree": "^0.0.46",
    "@types/flat": "^5.0.2",
    "@types/fuzzaldrin": "^2.1.3",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.178",
    "@types/memoizee": "^0.4.5",
    "@types/node": "^12.12.5",
    "@types/picomatch": "^2.3.0",
    "@types/semver": "^7.3.4",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "broccoli-test-helper": "^2.0.0",
    "cross-spawn": "7.0.3",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "^6.0.0",
    "jest": "^27.5.1",
    "lint-staged": "12.3.7",
    "node-polyfill-webpack-plugin": "1.1.4",
    "nyc": "15.1.0",
    "path-browserify": "1.0.1",
    "prettier": "2.6.1",
    "rimraf": "3.0.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.3",
    "ts-node": "^8.10.2",
    "typescript": "^4.6.3",
    "webpack": "^5.44.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.7.2"
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": true,
    "printWidth": 160,
    "singleQuote": true,
    "arrowParens": "always",
    "endOfLine": "auto",
    "parser": "typescript"
  },
  "scripts": {
    "clean": "rimraf lib/",
    "build:bundle:node": "webpack --mode production",
    "build:bundle:worker": "webpack --mode production && node ./fix-worker-bundle.js",
    "compile": "tsc --skipLibCheck -p .",
    "lint": "eslint \"./{src,test}/**/*.ts\"",
    "prepublish": "yarn clean && yarn compile",
    "test": "jest",
    "test:coverage": "rimraf inst/ && nyc instrument lib inst && nyc --no-clean --reporter=clover --reporter=json --reporter=lcov yarn run jest --no-coverage",
    "watch": "tsc --watch -p .",
    "prepare": "husky install",
    "test:worker-bundle": "npx http-server ."
  },
  "volta": {
    "node": "14.21.1",
    "yarn": "1.22.4"
  }
}
